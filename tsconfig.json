{
  "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es2015",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
        //   "*": [
        //       "node_modules/*",
        //   ],
        "@routes/*": ["src/routes/*"],
      },
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
  },
  "include": [
      "src/**/*"
  ]
}